<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>PretragaKlijentaServlet</servlet-name>
        <servlet-class>controller.PretragaKlijentaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KlijentUnosServlet</servlet-name>
        <servlet-class>controller.KlijentUnosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PretragaRacunaServlet</servlet-name>
        <servlet-class>controller.PretragaRacunaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UnosRacunaServlet</servlet-name>
        <servlet-class>controller.UnosRacunaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PretragaKlijentaServlet2</servlet-name>
        <servlet-class>controller.PretragaKlijentaServlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IzmenaKlijentaServlet</servlet-name>
        <servlet-class>controller.IzmenaKlijentaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IzmenaRacunaServlet</servlet-name>
        <servlet-class>controller.IzmenaRacunaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NadjiServlet</servlet-name>
        <servlet-class>controller.NadjiServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PretragaKlijentaServlet</servlet-name>
        <url-pattern>/PretragaKlijentaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KlijentUnosServlet</servlet-name>
        <url-pattern>/KlijentUnosServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PretragaRacunaServlet</servlet-name>
        <url-pattern>/PretragaRacunaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnosRacunaServlet</servlet-name>
        <url-pattern>/UnosRacunaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PretragaKlijentaServlet2</servlet-name>
        <url-pattern>/PretragaKlijentaServlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IzmenaKlijentaServlet</servlet-name>
        <url-pattern>/IzmenaKlijentaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IzmenaRacunaServlet</servlet-name>
        <url-pattern>/IzmenaRacunaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NadjiServlet</servlet-name>
        <url-pattern>/NadjiServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
